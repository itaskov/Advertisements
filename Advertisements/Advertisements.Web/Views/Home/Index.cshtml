@model Advertisements.Infrastructures.ViewModels.Home.IndexViewModel

@{
    ViewBag.Title = "Ads - Home";
}

<header>
    <h2 class="text-center">@ViewBag.Title</h2>
</header>

<div class="jumbotron">
    <div id="all-ads">
        @Html.Partial("_Advertisement", @Model.AdsIndexViewModel)
    </div>
</div>

@section Navigation
{
    @Html.Partial("_Navigation")
}

@section RightSideBar
{
    @Html.Partial("_RightSideBar", Model.RightSideBarViewModel)
}